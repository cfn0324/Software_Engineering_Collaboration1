t\" label=\"组长学号\" prop=\"zuzhangxuehao\">\n              <el-input v-model=\"ruleForm.zuzhangxuehao\" \n                placeholder=\"组长学号\" readonly></el-input>\n          </el-form-item>\n        </div>\n      </el-col>\n      <el-col :span=\"12\">\n        <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"组长姓名\" prop=\"zuzhangxingming\">\n          <el-input v-model=\"ruleForm.zuzhangxingming\" \n              placeholder=\"组长姓名\" clearable  :readonly=\"ro.zuzhangxingming\"></el-input>\n        </el-form-item>\n        <div v-else>\n          <el-form-item class=\"input\" label=\"组长姓名\" prop=\"zuzhangxingming\">\n              <el-input v-model=\"ruleForm.zuzhangxingming\" \n                placeholder=\"组长姓名\" readonly></el-input>\n          </el-form-item>\n        </div>\n      </el-col>\n      <el-col :span=\"12\">\n        <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"小组人数\" prop=\"xiaozurenshu\">\n          <el-input v-model=\"ruleForm.xiaozurenshu\" \n              placeholder=\"小组人数\" clearable  :readonly=\"ro.xiaozurenshu\"></el-input>\n        </el-form-item>\n        <div v-else>\n          <el-form-item class=\"input\" label=\"小组人数\" prop=\"xiaozurenshu\">\n              <el-input v-model=\"ruleForm.xiaozurenshu\" \n                placeholder=\"小组人数\" readonly></el-input>\n          </el-form-item>\n        </div>\n      </el-col>\n      <el-col :span=\"12\">\n        <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"学号\" prop=\"xuehao\">\n          <el-input v-model=\"ruleForm.xuehao\" \n              placeholder=\"学号\" clearable  :readonly=\"ro.xuehao\"></el-input>\n        </el-form-item>\n        <div v-else>\n          <el-form-item class=\"input\" label=\"学号\" prop=\"xuehao\">\n              <el-input v-model=\"ruleForm.xuehao\" \n                placeholder=\"学号\" readonly></el-input>\n          </el-form-item>\n        </div>\n      </el-col>\n      <el-col :span=\"12\">\n        <el-form-item class=\"inp